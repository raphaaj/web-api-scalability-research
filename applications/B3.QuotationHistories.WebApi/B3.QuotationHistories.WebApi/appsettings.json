{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": true,
        "SingleLine": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff ",
        "UseUtcTimestamp": false
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "B3QuotationHistories2024Connection": ""
  },
  "CacheSettings": {
    "UseCacheForTopAssetsWithHighestNegotiatedVolume": false,
    "NumberOfAssetsToCache": 100
  }
}
